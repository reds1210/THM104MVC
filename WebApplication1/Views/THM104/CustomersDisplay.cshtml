@model List<CustomersDisplayViewModel>@section Styles {    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.7/css/jquery.dataTables.css" />}@section Scripts
{    <script src="https://cdn.datatables.net/1.13.7/js/jquery.dataTables.js"></script>    <script>        $("#mytable").DataTable();    </script>}<table id="mytable">    <thead>        <tr>            <th>CustomerId</th>            <th>CompanyName</th>            <th>Region</th>            <th>City</th>            <th>Address</th>        </tr>    </thead>    <tbody>        @{            foreach (var prod in Model)
            {                <tr>                    <th>@prod.CompanyName</th>                    <th>@prod.CustomerId</th>                    <th>@prod.Region</th>                    <th>@prod.City</th>                    <th>@prod.Address</th>                </tr>            }        }    </tbody></table>